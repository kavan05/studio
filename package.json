    "csv-parse": "^5.5.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.33",
    "genkit-cli": "^1.20.0",
    "jsdom": "^27.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
}
